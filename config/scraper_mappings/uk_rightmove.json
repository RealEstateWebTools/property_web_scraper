[
{
  "name": "uk_rightmove",
  "expectedExtractionRate": 0.85,
  "defaultValues":
  {
    "country":
    {
      "value": "UK"
    },
    "area_unit":
    {
      "value": "sqft"
    },
    "currency":
    {
      "value": "GBP"
    },
    "for_rent_short_term": {
      "value": "false"
    },
    "locale_code": {
      "value": "en"
    }
  },
  "intFields": {
    "count_bedrooms": {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.bedrooms"
    },
    "count_bathrooms": {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.bathrooms"
    }
  },
  "floatFields":
  {
    "price_float":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "analyticsInfo.analyticsProperty.price"
    },
    "latitude":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.location.latitude"
    },
    "longitude":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.location.longitude"
    }
  },
  "textFields":
  {
    "reference": {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.id"
    },
    "title":
    {
      "cssLocator": "title",
      "fallbacks": [
        { "cssLocator": "meta[property='og:title']", "cssAttr": "content" },
        { "cssLocator": "h1" }
      ]
    },
    "description":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.text.description",
      "fallbacks": [
        { "cssLocator": "meta[name='description']", "cssAttr": "content" }
      ]
    },
    "price_string":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.prices.primaryPrice"
    },
    "postal_code":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "analyticsInfo.analyticsProperty.postcode"
    },
    "address_string":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.address.displayAddress"
    },
    "region":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.address.ukCountry"
    },
    "property_type":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.propertySubType"
    },
    "main_image_url":
    {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "meta[name='twitter:image']", "cssAttr": "content" }
      ]
    },
    "tenure":
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.tenure.tenureType"
    }
  },
  "images": [
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.images",
      "cssAttr": "url"
    }
  ],
  "features": [
    {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.keyFeatures"
    }
  ],
  "booleanFields": {
    "for_sale": {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.channel",
      "evaluator": "include?",
      "evaluatorParam": "BUY"
    },
    "for_rent": {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.channel",
      "evaluator": "include?",
      "evaluatorParam": "LET"
    },
    "for_rent_long_term": {
      "scriptJsonVar": "PAGE_MODEL",
      "scriptJsonPath": "propertyData.channel",
      "evaluator": "include?",
      "evaluatorParam": "LET"
    }
  }

}]
