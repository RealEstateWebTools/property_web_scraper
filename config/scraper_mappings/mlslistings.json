[{
  "name": "mlslistings",
  "defaultValues": {
    "country": {
      "value": "USA"
    },
    "area_unit": {
      "value": "sqft"
    },
    "currency": {
      "value": "USD"
    }
  },
  "floatFields": {
    "constructed_area": {
      "cssLocator": "#quickDetails > div > div:nth-child(3) > p:nth-child(1) > span",
      "stripPunct": "true"
    },
    "price_float": {
      "cssLocator": "#propertyAddress",
      "splitTextCharacter": " ",
      "splitTextArrayId": "0",
      "stripFirstChar": "true",
      "stripPunct": "true"
    },
    "longitude": {
      "xpath": "//meta[@property='place:location:longitude']/@content"
    },
    "latitude": {
      "xpath": "//meta[@property='place:location:latitude']/@content"
    }
  },
  "textFields": {
    "reference": {
      "cssLocator": "#quickDetails > div > div:nth-child(3) > p:nth-child(3) > span"
    },
    "title": {
      "xpath": "//meta[@property='og:title']/@content"
    },
    "description": {
      "cssLocator": "#propertyRemarks"
    },
    "price_string": {
      "cssLocator": "#propertyAddress",
      "splitTextCharacter": " ",
      "splitTextArrayId": "0"
    },
    "count_bedrooms": {
      "cssLocator": "#quickDetails > div > div:nth-child(2) > p:nth-child(1)",
      "splitTextCharacter": " ",
      "splitTextArrayId": "1"
    }
  },
  "booleanFields": {
    "for_sale": {
      "xpath": "//meta[@property='og:title']/@content",
      "evaluator": "include?",
      "evaluatorParam": "For Sale"
    }
  }
}]
