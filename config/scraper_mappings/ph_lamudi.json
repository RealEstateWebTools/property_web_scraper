[{
  "name": "ph_lamudi",
  "expectedExtractionRate": 0.65,

  // lamudi.com.ph â€” Philippines' leading property portal
  // Uses JSON-LD and meta tags

  "defaultValues": {
    "country":            { "value": "Philippines" },
    "currency":           { "value": "PHP" },
    "area_unit":          { "value": "sqmt" },
    "locale_code":        { "value": "en-PH" },
    "for_rent_short_term":{ "value": "false" }
  },

  "images": [{
    "cssLocator": "img[src*='lamudi']",
    "cssAttr": "src"
  }],

  "intFields": {
    "count_bedrooms": {
      "jsonLdPath": "numberOfBedrooms",
      "fallbacks": [
        { "cssLocator": "[data-attr='bedrooms'], .listing-attr-bedrooms", "modifiers": ["digitsOnly"] }
      ]
    },
    "count_bathrooms": {
      "jsonLdPath": "numberOfBathroomsTotal",
      "fallbacks": [
        { "cssLocator": "[data-attr='bathrooms'], .listing-attr-bathrooms", "modifiers": ["digitsOnly"] }
      ]
    },
    "count_parking": {
      "cssLocator": "[data-attr='parking'], .listing-attr-parking",
      "modifiers": ["digitsOnly"]
    }
  },

  "floatFields": {
    "price_float": {
      "jsonLdPath": "offers.price",
      "fallbacks": [
        { "cssLocator": ".listing-price, [data-attr='price']", "modifiers": ["digitsOnly"] }
      ]
    },
    "constructed_area": {
      "jsonLdPath": "floorSize.value",
      "fallbacks": [
        { "cssLocator": "[data-attr='floor-area']", "modifiers": ["digitsOnly"] }
      ]
    },
    "plot_area": {
      "jsonLdPath": "lotSize.value",
      "fallbacks": [
        { "cssLocator": "[data-attr='lot-area']", "modifiers": ["digitsOnly"] }
      ]
    },
    "latitude": {
      "jsonLdPath": "geo.latitude"
    },
    "longitude": {
      "jsonLdPath": "geo.longitude"
    }
  },

  "textFields": {
    "title": {
      "cssLocator": "meta[property='og:title']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "head > title" }
      ]
    },
    "description": {
      "cssLocator": "meta[property='og:description']",
      "cssAttr": "content"
    },
    "price_string": {
      "cssLocator": ".listing-price, [data-attr='price']",
      "fallbacks": [
        { "jsonLdPath": "offers.price" }
      ]
    },
    "address_string": {
      "jsonLdPath": "address.streetAddress",
      "fallbacks": [
        { "cssLocator": ".listing-location" }
      ]
    },
    "city": {
      "jsonLdPath": "address.addressLocality"
    },
    "region": {
      "jsonLdPath": "address.addressRegion"
    },
    "main_image_url": {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content"
    },
    "property_type": {
      "jsonLdPath": "@type"
    },
    "reference": {
      "urlPathPart": "-1"
    }
  },

  "booleanFields": {
    "for_sale": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "buy"
    },
    "for_rent": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "rent"
    },
    "for_rent_long_term": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "rent"
    }
  }
}]
