[{
  "name": "vn_batdongsan",
  "expectedExtractionRate": 0.60,

  // batdongsan.com.vn â€” Vietnam's largest real estate portal
  // Primary data source: JSON-LD (SingleFamilyResidence/Product)
  // Secondary: CSS selectors and meta tags
  // URL patterns: /ban-/ (sell) vs /cho-thue/ (rent)

  "defaultValues": {
    "country":            { "value": "Vietnam" },
    "currency":           { "value": "VND" },
    "area_unit":          { "value": "sqmt" },
    "locale_code":        { "value": "vi-VN" },
    "for_rent_short_term":{ "value": "false" }
  },

  "images": [{
    "cssLocator": "img[src*='batdongsan'], img[src*='file4.batdongsan']",
    "cssAttr": "src"
  }],

  "intFields": {
    "count_bedrooms": {
      "jsonLdPath": "numberOfBedrooms",
      "fallbacks": [
        { "cssLocator": ".re__pr-specs--content-item:has(.re__icon-bedroom) .re__pr-specs--content-item-value, [class*='bedroom']", "modifiers": ["digitsOnly"] }
      ]
    },
    "count_bathrooms": {
      "jsonLdPath": "numberOfBathroomsTotal",
      "fallbacks": [
        { "cssLocator": ".re__pr-specs--content-item:has(.re__icon-bathroom) .re__pr-specs--content-item-value, [class*='bathroom']", "modifiers": ["digitsOnly"] }
      ]
    }
  },

  "floatFields": {
    "price_float": {
      "jsonLdPath": "offers.price",
      "fallbacks": [
        { "cssLocator": ".price, .re__pr-short-info-item--price .value", "modifiers": ["digitsOnly"] }
      ]
    },
    "constructed_area": {
      "jsonLdPath": "floorSize.value",
      "fallbacks": [
        { "cssLocator": ".re__pr-specs--content-item:has(.re__icon-area) .re__pr-specs--content-item-value, [class*='area'] .value", "modifiers": ["digitsOnly"] }
      ]
    },
    "latitude": {
      "jsonLdPath": "geo.latitude"
    },
    "longitude": {
      "jsonLdPath": "geo.longitude"
    }
  },

  "textFields": {
    "title": {
      "cssLocator": "meta[property='og:title']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "head > title" }
      ]
    },
    "description": {
      "cssLocator": "meta[property='og:description']",
      "cssAttr": "content"
    },
    "price_string": {
      "cssLocator": ".price, .re__pr-short-info-item--price .value",
      "fallbacks": [
        { "jsonLdPath": "offers.price" }
      ]
    },
    "address_string": {
      "jsonLdPath": "address.streetAddress",
      "fallbacks": [
        { "cssLocator": ".re__pr-short-info-item--address .value, [class*='address']" }
      ]
    },
    "city": {
      "jsonLdPath": "address.addressLocality"
    },
    "region": {
      "jsonLdPath": "address.addressRegion"
    },
    "main_image_url": {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content"
    },
    "property_type": {
      "jsonLdPath": "@type"
    },
    "reference": {
      "urlPathPart": "-1",
      "regex": "(\\d+)",
      "regexGroup": 1
    }
  },

  "booleanFields": {
    "for_sale": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "ban-"
    },
    "for_rent": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "cho-thue"
    },
    "for_rent_long_term": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "cho-thue"
    }
  }
}]
