[{
  "name": "ie_daft",
  "expectedExtractionRate": 0.90,
  "defaultValues": {
    "country": { "value": "Ireland" },
    "area_unit": { "value": "sqm" },
    "currency": { "value": "EUR" },
    "for_rent_short_term": { "value": "false" },
    "locale_code": { "value": "en" }
  },
  "intFields": {
    "count_bedrooms": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.nonFormatted.beds"
    },
    "count_bathrooms": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.numBathrooms"
    },
    "constructed_area": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.floorArea.value"
    }
  },
  "floatFields": {
    "price_float": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.nonFormatted.price"
    },
    "latitude": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.point.coordinates.1"
    },
    "longitude": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.point.coordinates.0"
    }
  },
  "textFields": {
    "reference": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.id"
    },
    "title": {
      "cssLocator": "title",
      "fallbacks": [
        { "cssLocator": "meta[property='og:title']", "cssAttr": "content" },
        { "cssLocator": "h1" }
      ]
    },
    "description": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.description",
      "fallbacks": [
        { "cssLocator": "meta[property='og:description']", "cssAttr": "content" },
        { "cssLocator": "meta[name='description']", "cssAttr": "content" }
      ]
    },
    "price_string": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.price"
    },
    "address_string": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.title"
    },
    "property_type": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.propertyType"
    },
    "city": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.addressDetails.addressLocality"
    },
    "region": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.addressDetails.addressRegion"
    },
    "main_image_url": {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "meta[name='twitter:image']", "cssAttr": "content" }
      ]
    }
  },
  "images": [{
    "scriptJsonVar": "__NEXT_DATA__",
    "scriptJsonPath": "props.pageProps.listing.media.images",
    "cssAttr": "size720x480"
  }],
  "booleanFields": {
    "for_sale": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.category",
      "evaluator": "include?",
      "evaluatorParam": "Buy"
    },
    "for_rent": {
      "scriptJsonVar": "__NEXT_DATA__",
      "scriptJsonPath": "props.pageProps.listing.category",
      "evaluator": "include?",
      "evaluatorParam": "Rent"
    }
  }
