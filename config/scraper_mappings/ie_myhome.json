[{
  "name": "ie_myhome",
  "expectedExtractionRate": 0.65,

  // myhome.ie â€” Ireland's #2 property portal (The Irish Times)
  // Uses JSON-LD and meta tags

  "defaultValues": {
    "country":            { "value": "Ireland" },
    "currency":           { "value": "EUR" },
    "area_unit":          { "value": "sqmt" },
    "locale_code":        { "value": "en-IE" },
    "for_rent_short_term":{ "value": "false" }
  },

  "images": [{
    "cssLocator": "img[src*='myhome'], img[src*='media.myhome']",
    "cssAttr": "src"
  }],

  "intFields": {
    "count_bedrooms": {
      "jsonLdPath": "numberOfBedrooms",
      "fallbacks": [
        { "cssLocator": ".info-strip--bedrooms, [data-testid='bedrooms']", "modifiers": ["digitsOnly"] }
      ]
    },
    "count_bathrooms": {
      "jsonLdPath": "numberOfBathroomsTotal",
      "fallbacks": [
        { "cssLocator": ".info-strip--bathrooms, [data-testid='bathrooms']", "modifiers": ["digitsOnly"] }
      ]
    }
  },

  "floatFields": {
    "price_float": {
      "jsonLdPath": "offers.price",
      "fallbacks": [
        { "cssLocator": ".brochure__price, .price-info__price", "modifiers": ["digitsOnly"] }
      ]
    },
    "constructed_area": {
      "jsonLdPath": "floorSize.value",
      "fallbacks": [
        { "cssLocator": ".info-strip--floor-area", "modifiers": ["digitsOnly"] }
      ]
    },
    "latitude": {
      "jsonLdPath": "geo.latitude"
    },
    "longitude": {
      "jsonLdPath": "geo.longitude"
    }
  },

  "textFields": {
    "title": {
      "cssLocator": "meta[property='og:title']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "head > title" }
      ]
    },
    "description": {
      "cssLocator": "meta[property='og:description']",
      "cssAttr": "content"
    },
    "price_string": {
      "cssLocator": ".brochure__price, .price-info__price",
      "fallbacks": [
        { "jsonLdPath": "offers.price" }
      ]
    },
    "address_string": {
      "jsonLdPath": "address.streetAddress",
      "fallbacks": [
        { "cssLocator": ".brochure__address" }
      ]
    },
    "city": {
      "jsonLdPath": "address.addressLocality"
    },
    "region": {
      "jsonLdPath": "address.addressRegion"
    },
    "main_image_url": {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content"
    },
    "property_type": {
      "jsonLdPath": "@type"
    },
    "agent_name": {
      "cssLocator": ".brochure__agent-name"
    },
    "reference": {
      "urlPathPart": "-1"
    }
  },

  "booleanFields": {
    "for_sale": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "sale"
    },
    "for_rent": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "rent"
    },
    "for_rent_long_term": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "rent"
    }
  }
}]
