[{
  "name": "fi_oikotie",
  "expectedExtractionRate": 0.60,

  // oikotie.fi â€” Finland's leading property portal
  // Uses JSON-LD and meta tags

  "defaultValues": {
    "country":            { "value": "Finland" },
    "currency":           { "value": "EUR" },
    "area_unit":          { "value": "sqmt" },
    "locale_code":        { "value": "fi-FI" },
    "for_rent_short_term":{ "value": "false" }
  },

  "images": [{
    "cssLocator": "img[src*='oikotie']",
    "cssAttr": "src"
  }],

  "intFields": {
    "count_bedrooms": {
      "jsonLdPath": "numberOfRooms",
      "fallbacks": [
        { "cssLocator": "[data-testid='rooms']", "modifiers": ["digitsOnly"] }
      ]
    },
    "year_construction": {
      "jsonLdPath": "yearBuilt"
    }
  },

  "floatFields": {
    "price_float": {
      "jsonLdPath": "offers.price",
      "fallbacks": [
        { "cssLocator": "[data-testid='price']", "modifiers": ["digitsOnly"] }
      ]
    },
    "constructed_area": {
      "jsonLdPath": "floorSize.value",
      "fallbacks": [
        { "cssLocator": "[data-testid='living-area']", "modifiers": ["digitsOnly"] }
      ]
    },
    "latitude": {
      "jsonLdPath": "geo.latitude"
    },
    "longitude": {
      "jsonLdPath": "geo.longitude"
    }
  },

  "textFields": {
    "title": {
      "cssLocator": "meta[property='og:title']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "head > title" }
      ]
    },
    "description": {
      "cssLocator": "meta[property='og:description']",
      "cssAttr": "content"
    },
    "price_string": {
      "cssLocator": "[data-testid='price']",
      "fallbacks": [
        { "jsonLdPath": "offers.price" }
      ]
    },
    "address_string": {
      "jsonLdPath": "address.streetAddress"
    },
    "city": {
      "jsonLdPath": "address.addressLocality"
    },
    "region": {
      "jsonLdPath": "address.addressRegion"
    },
    "postal_code": {
      "jsonLdPath": "address.postalCode"
    },
    "main_image_url": {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content"
    },
    "reference": {
      "urlPathPart": "-1"
    }
  },

  "booleanFields": {
    "for_sale": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "myytavat"
    },
    "for_rent": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "vuokrattavat"
    },
    "for_rent_long_term": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "vuokrattavat"
    }
  }
}]
