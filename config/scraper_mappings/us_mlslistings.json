[{
  "name": "us_mlslistings",
  "expectedExtractionRate": 0.95,
  "unRetrievedFields": [
    "count_bathrooms"
  ],
  "defaultValues": {
    "country": {
      "value": "USA"
    },
    "area_unit": {
      "value": "sqft"
    },
    "currency": {
      "value": "USD"
    }
  },
  "intFields": {
    "year_construction": {
      "scriptJsonVar": "PropertyDetailsByMLSNumber",
      "scriptJsonPath": "propertyDetailResults.yearBuilt"
    }
  },
  "floatFields": {
    "constructed_area": {
      "cssLocator": "#quickDetails > div > div:nth-child(3) > p:nth-child(1) > span",
      "stripPunct": "true"
    },
    "price_float": {
      "cssLocator": "#propertyAddress",
      "splitTextCharacter": " ",
      "splitTextArrayId": "0",
      "stripFirstChar": "true",
      "stripPunct": "true"
    },
    "longitude": {
      "cssLocator": "meta[property='place:location:longitude']",
      "cssAttr": "content"
    },
    "latitude": {
      "cssLocator": "meta[property='place:location:latitude']",
      "cssAttr": "content"
    }
  },
  "textFields": {
    "reference": {
      "cssLocator": "#quickDetails > div > div:nth-child(3) > p:nth-child(3) > span"
    },
    "title": {
      "cssLocator": "meta[property='og:title']",
      "cssAttr": "content"
    },
    "description": {
      "cssLocator": "#propertyRemarks"
    },
    "price_string": {
      "cssLocator": "#propertyAddress",
      "splitTextCharacter": " ",
      "splitTextArrayId": "0"
    },
    // "count_bathrooms": {
    //   "xpath": "//meta[@property='mlsl_fb:baths']/@content"
    // },
    "count_bedrooms": {
      "cssLocator": "meta[property='mlsl_fb:beds']",
      "cssAttr": "content"
      // "splitTextCharacter": " ",
      // "splitTextArrayId": "1"
    },
    "main_image_url": {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content"
    },
    "property_type": {
      "scriptJsonVar": "PropertyDetailsByMLSNumber",
      "scriptJsonPath": "propertyDetailResults.subClass"
    }
  },
  "booleanFields": {
    "for_sale": {
      "cssLocator": "meta[property='og:title']",
      "cssAttr": "content",
      "evaluator": "include?",
      "evaluatorParam": "For Sale"
    }
  }
}]
