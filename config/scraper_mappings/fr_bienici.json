[{
  "name": "fr_bienici",
  "expectedExtractionRate": 0.60,

  // bienici.com â€” France's #3 property portal
  // Uses JSON-LD and CSS fallbacks
  // URL paths: "achat" (buy) vs "location" (rent)

  "defaultValues": {
    "country":            { "value": "France" },
    "currency":           { "value": "EUR" },
    "area_unit":          { "value": "sqmt" },
    "locale_code":        { "value": "fr-FR" },
    "for_rent_short_term":{ "value": "false" }
  },

  "images": [{
    "cssLocator": "meta[property='og:image']",
    "cssAttr": "content"
  }],

  "intFields": {
    "count_bedrooms": {
      "jsonLdPath": "numberOfBedrooms",
      "fallbacks": [
        { "cssLocator": ".allDetails__bedrooms, [data-testid='bedrooms']", "modifiers": ["digitsOnly"] }
      ]
    },
    "count_bathrooms": {
      "jsonLdPath": "numberOfBathroomsTotal",
      "fallbacks": [
        { "cssLocator": ".allDetails__bathrooms, [data-testid='bathrooms']", "modifiers": ["digitsOnly"] }
      ]
    }
  },

  "floatFields": {
    "price_float": {
      "jsonLdPath": "offers.price",
      "fallbacks": [
        { "cssLocator": ".ad-price__the-price, .new-price", "modifiers": ["digitsOnly"] }
      ]
    },
    "constructed_area": {
      "jsonLdPath": "floorSize.value",
      "fallbacks": [
        { "cssLocator": ".allDetails__surface, .ad-overview-details__surface-value", "modifiers": ["digitsOnly"] }
      ]
    },
    "latitude": {
      "jsonLdPath": "geo.latitude"
    },
    "longitude": {
      "jsonLdPath": "geo.longitude"
    }
  },

  "textFields": {
    "title": {
      "cssLocator": "meta[property='og:title']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "head > title" }
      ]
    },
    "description": {
      "cssLocator": "meta[property='og:description']",
      "cssAttr": "content"
    },
    "price_string": {
      "cssLocator": ".ad-price__the-price, .new-price",
      "fallbacks": [
        { "jsonLdPath": "offers.price" }
      ]
    },
    "address_string": {
      "jsonLdPath": "address.streetAddress",
      "fallbacks": [
        { "cssLocator": ".ad-overview-details__address, .fullAddress" }
      ]
    },
    "city": {
      "jsonLdPath": "address.addressLocality"
    },
    "region": {
      "jsonLdPath": "address.addressRegion"
    },
    "main_image_url": {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content"
    },
    "property_type": {
      "jsonLdPath": "@type"
    },
    "reference": {
      "urlPathPart": "-1"
    }
  },

  "booleanFields": {
    "for_sale": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "achat"
    },
    "for_rent": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "location"
    },
    "for_rent_long_term": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "location"
    }
  }
}]
