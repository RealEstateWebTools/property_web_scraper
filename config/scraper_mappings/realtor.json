[
{
  "name": "realtor",
  "defaultValues":
  {
    "country":
    {
      "value": "USA"
    },
    "area_unit":
    {
      "value": "sqft"
    },
    "currency":
    {
      "value": "USD"
    },
    "for_rent_short_term": {
      "value": "false"
    }
  },
  "floatFields":
  {
    "constructed_area":
    {
      "cssLocator": "#ldp-property-meta > ul > li:nth-child(3) > span",
      "stripPunct": "true"
    },
    "price_float":
    {
      "cssLocator": "#ldp-pricewrap > div > div > span",
      "stripFirstChar": "true",
      "stripPunct": "true"
    },
    "longitude":
    {
      "cssLocator": "meta[property='place:location:longitude']",
      "cssAttr": "content"
    },
    "latitude":
    {
      "cssLocator": "meta[property='place:location:latitude']",
      "cssAttr": "content"
    }
  },
  "intFields": {
    "count_bedrooms": {
      "cssLocator": "#ldp-property-meta li[data-label='property-meta-beds']"
    },
    "count_bathrooms": {
      "cssLocator": "#ldp-property-meta li[data-label='property-meta-bath']"
    } 
  },
  "textFields":
  {
    "reference":
    {
      // "xpath": "//div[@data-listingid]"
      "cssLocator": ".js-tracking",
      "cssAttr": "data-listingid"
    },
    "title":
    {
      "cssLocator": "#ldp-address > h1 > span:nth-child(1)"
    },
    "description":
    {
      "cssLocator": "meta[name='description']",
      "cssAttr": "content"
    },
    "price_string":
    {
      "cssLocator": "#ldp-pricewrap > div > div > span"
    },
    "postal_code":
    {
      "cssLocator": "meta[property='og:postal-code']",
      "cssAttr": "content"
    },
    "city":
    {
      "cssLocator": "meta[property='og:locality']",
      "cssAttr": "content"
    },
    "region":
    {
      "cssLocator": "meta[property='og:region']",
      "cssAttr": "content"
    },
    "address_string":
    {
      "cssLocator": "meta[property='og:street-address']",
      "cssAttr": "content"
    },
    "main_image_url":
    {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content"
    }
  },
  "booleanFields":
  {
    "for_rent_long_term":
    {
      "cssLocator": "span[data-label='property-meta-status']",
      "evaluator": "include?",
      "caseInsensitive": true,
      "evaluatorParam": "rent"
    },
    "for_rent":
    {
      "cssLocator": "span[data-label='property-meta-status']",
      "evaluator": "include?",
      "caseInsensitive": true,
      "evaluatorParam": "rent"
    },
    "for_sale":
    {
      "cssLocator": "span[data-label='property-meta-status']",
      "evaluator": "include?",
      "caseInsensitive": true,
      "evaluatorParam": "sale"
    }
  },
  "images": [
  {
    "cssLocator": "#ldpPhotoGallery img",
    "xmlAttr": "data-src"
  }]

}]