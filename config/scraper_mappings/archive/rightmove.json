[
{
  "name": "rightmove",
  "version": 1,
  "last_checked": "2026-02-20",
  "expectedExtractionRate": 0.90,
  "defaultValues":
  {
    "country":
    {
      "value": "UK"
    },
    "area_unit":
    {
      "value": "sqft"
    },
    "currency":
    {
      "value": "GBP"
    },
    "for_rent_short_term": {
      "value": "false"
    },
    "locale_code": {
      "value": "en"
    }
  },
  "intFields": {
    "count_bedrooms": {
      "scriptRegEx": "beds[^\\,]*",
      "splitTextCharacter": ":",
      "splitTextArrayId": "1"
    }
  },
  "floatFields":
  {
    "price_float":
    {
      "cssLocator": "#propertyHeaderPrice > strong",
      "stripFirstChar": "true",
      "stripPunct": "true"
    },
    "latitude":
    {
      // below is actually "latitude:[^\,]*" but had to escape the \
      "scriptRegEx": "latitude[^\\}]*",
      "splitTextCharacter": ":",
      "splitTextArrayId": "1"
    },
    "longitude":
    {
      "scriptRegEx": "longitude[^\\}]*",
      "splitTextCharacter": ":",
      "splitTextArrayId": "1"
    }
  },
  "textFields":
  {
    "reference": {
      "urlPathPart": "2",
      "splitTextCharacter": "-",
      "splitTextArrayId": "1",
      "stripString": ".html"
    },
    "title":
    {
      "cssLocator": "title",
      "fallbacks": [
        { "cssLocator": "h1[itemprop='name']" },
        { "cssLocator": "h1" }
      ]
    },
    "description":
    {
      "cssLocator": "p[itemprop='description']",
      "fallbacks": [
        { "cssLocator": ".sect-description p" },
        { "cssLocator": "[data-testid='truncated_text_container']" }
      ]
    },
    "price_string":
    {
      "cssLocator": "#propertyHeaderPrice > strong",
      "fallbacks": [
        { "cssLocator": "[itemprop='price']", "cssAttr": "content" },
        { "cssLocator": ".property-header-price strong" }
      ]
    },
    "postal_code":
    {
      "scriptRegEx": "postcode[^\\,]*",
      "splitTextCharacter": "\"",
      "splitTextArrayId": "2"
    },
    "address_string":
    {
      "cssLocator": "meta[itemprop='streetAddress']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "[data-testid='address-label']" },
        { "cssLocator": ".property-header-bedroom-and-price address" }
      ]
    },
    "street_address":
    {
      "cssLocator": "meta[itemprop='streetAddress']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "[data-testid='address-label']" }
      ]
    },
    "main_image_url":
    {
      "cssLocator": "meta[name='twitter:image:src']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "meta[property='og:image']", "cssAttr": "content" }
      ]
    }
  },
  "images": [
    {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content",
      "stripString": "_max_135x100"
    }
  ],
  "booleanFields": {
    "for_rent": {
      "urlPathPart": "1",
      "evaluator": "include?",
      "evaluatorParam": "property-to-rent"
    },
    "for_rent_long_term": {
      "urlPathPart": "1",
      "evaluator": "include?",
      "evaluatorParam": "property-to-rent"
    },
    "for_sale": {
      "urlPathPart": "1",
      "evaluator": "include?",
      "evaluatorParam": "property-for-sale"
    }
  }

}]