[{
  "name": "ua_lun",
  "expectedExtractionRate": 0.60,

  // lun.ua — Ukraine's leading real estate platform
  // Primary data source: JSON-LD (RealEstateListing/Apartment)
  // Secondary: meta og tags, CSS selectors
  // URL patterns: /prodazha/ (sale) vs /arenda/ (rent)

  "defaultValues": {
    "country":            { "value": "Ukraine" },
    "currency":           { "value": "UAH" },
    "area_unit":          { "value": "sqmt" },
    "locale_code":        { "value": "uk-UA" },
    "for_rent_short_term":{ "value": "false" }
  },

  "images": [{
    "cssLocator": "img[src*='lun.ua'], img[src*='lunstatic'], meta[property='og:image']",
    "cssAttr": "src"
  }],

  "intFields": {
    "count_bedrooms": {
      "jsonLdPath": "numberOfBedrooms",
      "fallbacks": [
        { "cssLocator": "[data-testid='rooms-count'], .realty-content-layout__info-item:contains('Кімнат') .realty-content-layout__info-value", "modifiers": ["digitsOnly"] }
      ]
    },
    "count_bathrooms": {
      "jsonLdPath": "numberOfBathroomsTotal",
      "fallbacks": [
        { "cssLocator": "[data-testid='bathrooms-count'], .realty-content-layout__info-item:contains('Санвузл') .realty-content-layout__info-value", "modifiers": ["digitsOnly"] }
      ]
    }
  },

  "floatFields": {
    "price_float": {
      "jsonLdPath": "offers.price",
      "fallbacks": [
        { "cssLocator": "[data-testid='realty-price'], .realty-price__value", "modifiers": ["digitsOnly"] }
      ]
    },
    "constructed_area": {
      "jsonLdPath": "floorSize.value",
      "fallbacks": [
        { "cssLocator": "[data-testid='total-area'], .realty-content-layout__info-item:contains('Площа') .realty-content-layout__info-value", "modifiers": ["digitsOnly"] }
      ]
    },
    "latitude": {
      "jsonLdPath": "geo.latitude"
    },
    "longitude": {
      "jsonLdPath": "geo.longitude"
    }
  },

  "textFields": {
    "title": {
      "cssLocator": "meta[property='og:title']",
      "cssAttr": "content",
      "fallbacks": [
        { "cssLocator": "head > title" }
      ]
    },
    "description": {
      "cssLocator": "meta[property='og:description']",
      "cssAttr": "content"
    },
    "price_string": {
      "cssLocator": "[data-testid='realty-price'], .realty-price__value",
      "fallbacks": [
        { "jsonLdPath": "offers.price" }
      ]
    },
    "address_string": {
      "jsonLdPath": "address.streetAddress",
      "fallbacks": [
        { "cssLocator": "[data-testid='realty-address'], .realty-address" }
      ]
    },
    "city": {
      "jsonLdPath": "address.addressLocality"
    },
    "region": {
      "jsonLdPath": "address.addressRegion"
    },
    "main_image_url": {
      "cssLocator": "meta[property='og:image']",
      "cssAttr": "content"
    },
    "property_type": {
      "jsonLdPath": "@type"
    },
    "reference": {
      "urlPathPart": "-1",
      "regex": "(\\d+)",
      "regexGroup": 1
    }
  },

  "booleanFields": {
    "for_sale": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "prodazha"
    },
    "for_rent": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "arenda"
    },
    "for_rent_long_term": {
      "urlPathPart": "1",
      "evaluator":      "include?",
      "evaluatorParam": "arenda"
    }
  }
}]
